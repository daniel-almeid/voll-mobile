{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule": true,
    "lib": [
      "es6",
      "dom"
    ]
  },
  "include": [
    "src"
  ],
  "extends": "expo/tsconfig.base"
}
